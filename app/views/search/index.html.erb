<h3><%= @total %> Total Stores </h3>
<table class="search">
  <tr>
    <th>Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone Number</th>
    <th>Store Type</th>
  </tr>
  <% @stores.each do |store| %>
  <tr>
    <td><%= store["name"]%></td>
    <td><%= store["city"]%></td>
    <td><%= store["distance"]%></td>
    <td><%= store["phone"]%></td>
    <td><%= store["storeType"]%></td>
  </tr>
  <% end %>
  
</table>